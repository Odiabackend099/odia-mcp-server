{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/mcp",
      "dest": "/api/mcp.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}